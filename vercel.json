{
  "buildCommand": "cd vyntar-local-seo && npm install && npm run build",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    { "source": "/local-seo/:path*", "destination": "/vyntar-local-seo/:path*" },
    { "source": "/((?!local-seo).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
